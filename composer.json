{
    "name": "precast-framework/precast-skeleton",
    "description": "",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Gerd Christian Kunze",
            "email": "gerdchristian.kunze@googlemail.com"
        }
    ],
    "config": {
        "vendor-dir": "Vendor/Repository",
        "optimize-autoloader": true,
        "classmap-authoritative": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "packagist": false
        }
    ],
    "require": {
        "php": "~7.1",
        "twbs/bootstrap": "~v4.0.0-beta",
        "doctrine/orm": "~v2.5",
        "twig/twig": "~v2.4",
        "symfony/finder": "~v3.3",
        "symfony/yaml": "~v3.3",
        "laravel/framework": "~v5.5",
        "phpfastcache/phpfastcache": "~6.0",
        "filp/whoops": "~2.1",
        "phpunit/phpunit": "~6.5",
        "phpunit/dbunit": "~3.0",
        "roave/better-reflection": "dev-master",
        "phpdocumentor/reflection-docblock": "~4.1",
        "apigen/apigen": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PRECAST\\Application\\": "Application",
            "PRECAST\\Environment\\": "Environment",
            "PRECAST\\Facade\\": "Facade",
            "PRECAST\\Storage\\": "Storage",
            "PRECAST\\Vendor\\": "Vendor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PRECAST\\": "UnitTest"
        }
    }
}
